@model List<Category>
<h1>List OF Category</h1>
<div>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>Category Name</td>
                <td>Description</td>
                <td>Display Order</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var category in Model)
            {
                <tr>
                    <td>@category.Name </td>
                    <td>@category.Description</td>
                    <td>@category.DisplayOrder</td>
                </tr>
            }
        </tbody>
    </Table>
</div>